    "lis=[]\n",
    "\n",
    "hole = 0\n",
    "li=input()\n",
    "\n",
    "for item in li:\n",
    "  if(item in s):\n",
    "    if(item=='B'):\n",
    "      hole=hole+2\n",
    "\n",
    "    else:\n",
    "      hole=hole+1\n",
    "\n",
    "print(hole)"
   ]
